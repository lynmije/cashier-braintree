{
    "name": "lynmije/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": ["laravel", "braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Relyn Aruy",
            "email": "relynaruy@gmail.com"
        }
    ],
    "require": {
        
        "php": "^7.2|^7.3|^7.4",
        "illuminate/support": "^6.0",
        "illuminate/view": "^6.0",
        "illuminate/database": "^6.0",
        "illuminate/http": "^6.0",
        "braintree/braintree_php": "~4.6",
        "dompdf/dompdf": "^0.8",
        "illuminate/contracts": "^6.0",
        "illuminate/routing": "^6.0",
        "nesbot/carbon": "^2.16",
        "symfony/http-kernel": "~2.7|~3.0|~4.0|~5.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
